{
    "name": "makohawk/safemock",
    "description": "SafeMock: A PHP library for type-safe data validation, API mocking, and test data generation",
    "require": {
        "php": "^8.4"

    },
    "require-dev": {
        "phpunit/phpunit": "^12.3.8",
        "pestphp/pest": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "SafeMock\\": "src/"
    }
    },
    "autoload-dev": {
        "psr-4": {
            "SafeMock\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
